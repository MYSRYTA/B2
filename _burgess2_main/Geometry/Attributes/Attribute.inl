//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
void AttributeT<T>::setName ( const String &_name ) {
	this->name = _name;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
void AttributeT<T>::setSubName ( const String &_name ) {
	this->subName = _name;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
String AttributeT<T>::getName ()const {
	return this->name;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
String AttributeT<T>::getSubName ()const {
	return this->subName;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
void AttributeT<T>::setType ( u8 _type ) {
	this->type = _type;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
void AttributeT<T>::setValid ( Bool _enable ){
	this->enable = _enable;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
void AttributeT<T>::setDirty ( Bool dirt ) {
	this->dirty = dirt;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
u8 AttributeT<T>::getType ()const {
	return this->type;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
Bool AttributeT<T>::isValid ()  const{
	return this->enable;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
Bool AttributeT<T>::isDirty ()  const {
	return this->dirty;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
Bool AttributeT<T>::is ( const String &_name )const {
	return this->name == _name;
}

//------------------------------------------------------------------------------------------------------------------------------------------------
//!	    
//------------------------------------------------------------------------------------------------------------------------------------------------
template<typename T>
Bool AttributeT<T>::is ( const String &_name, const String &_subName )const {
	return this->name == _name && this->subName == _subName;
}
